pipeline{
    agent{
        node{
            label "linux&&java11"
        }
     }
    stages{
        stage('say hello'){
            steps{
                echo "hello from pipeline"   
            }

        }
    }
    post {
        always {
            echo " this is always been fun!"
        }
        success{
            echo " this is sucess!"
        }
        failure {
            echo " sorry yo fail the test"
        }
        cleanup {
            echo "will cleanup soon"
        }
    }
}
